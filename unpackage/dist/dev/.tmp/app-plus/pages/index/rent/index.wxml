<view class="app"><view class="nav"><block wx:for="{{array}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{['nav_round',(index==isShow)?'active':'']}}" bindtap="__e"><text>{{item}}</text><view class="huakuai"></view></view></block></view><view hidden="{{!(isShow===0)}}" class="goodsList"><block wx:for="{{arr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['arr','',index,'a']]]]]]]}}" class="listRound" bindtap="__e"><view style="height:360rpx;display:flex;flex-direction:column;justify-content:center;align-items:center;"><image class="roundImg" src="{{item.src}}" mode="widthFix"></image></view><view class="roundName">{{item.name}}</view><view class="roundBottom"><view class="roundPrice">{{item.price}}</view><view class="roundNumber">{{item.number}}</view></view></view></block></view></view>