
.listRound{
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	border-bottom:1rpx solid #E1E1E1;
	padding:32rpx 0 34rpx 0;
}
.img{
	margin-left:12rpx;
	width:176rpx;
	height:178rpx;
	margin-right:20rpx;
}
.text{
	margin-top:6rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.name{
	font-size:26rpx;
	color:#212121;
	margin-bottom: 14rpx;
	line-height: 36rpx;
}
.price{
	font-size:26rpx;
	color:#F90A0A;
	margin-bottom:16rpx;
	font-weight:bold;
	line-height: 42rpx;
}
.user{
	font-size:24rpx;
	color: #212121;
	border:0rpx solid red;
	line-height: 34rpx;
}

